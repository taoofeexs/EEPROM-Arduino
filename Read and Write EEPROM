============================WRITE==================================
#include <EEPROM.h>

int address = 0;  // Lokasi awal di EEPROM

void setup() {
    // Setup kode
}

void loop() {
    int dataToStore = 42;  // Data yang ingin disimpan

    EEPROM.write(address, dataToStore);

    // Tambahkan delay atau kode lain sesuai kebutuhan Anda
}
============================READ==================================
#include <EEPROM.h>

int address = 0;  // Lokasi awal di EEPROM

void setup() {
    // Setup kode
}

void loop() {
    int storedData = EEPROM.read(address);

    // Gunakan storedData sesuai kebutuhan Anda

    // Tambahkan delay atau kode lain sesuai kebutuhan Anda
}
